{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "cpx \"public/**\" \"public/**\" --clean && rollup -c && cpx extension/**/*.* public",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "format": "prettier --write --plugin-search-dir=. ./**/*.{svelte,js,html}"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@types/chrome": "^0.0.179",
    "cpx": "^1.5.0",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sirv-cli": "^2.0.0",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "he": "^1.2.0"
  }
}
