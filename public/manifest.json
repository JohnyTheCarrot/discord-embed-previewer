{
  "name": "Discord Embeds",
  "version": "1.0",
  "description": "Preview sites' Discord embeds for debugging purposes.",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*", "file://*.html"],
      "js": ["meta-reader.js"]
    }
  ],
  "manifest_version": 3
}
